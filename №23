#include <iostream>
using namespace std;
int main(){
	double A,B,C,x,y;
	cout << "Input number pA";
	cin >> A;
	cout << "Input number pB";
	cin >> B;
	cout << "Input number pC";
	cin >> C;
	x = B;
	B = A;
	y = C;
	C = x;
	A = y;
	cout << "A: " << A << endl;
	cout << "B: " << B << endl;
	cout << "C: " << C << endl;
}
