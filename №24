#include <iostream>
using namespace std;
int main(){
	double A,B,C,x,y;
	cout << "Input number A";
	cin >> A;
	cout << "Input number B ";
	cin >> B;
	cout << "Input number C";
	cin >> C;
	x = C;
	C = A;
	y = B;
	B = x;
	A = y;
	cout << "A" << A << endl;
	cout << "B" << B << endl;
	cout << "C" << C << endl;
}
